{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "warn",
        "no-underscore-dangle": "off",
        "no-unused-vars": "warn",
        "no-use-before-define": "off",
        "prefer-const": "off",
        "arrow-body-style": "off",
        "no-shadow": "warn"
    }
}
